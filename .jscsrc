{
  "preset": "google",
  "excludeFiles": [
    "node_modules",
    "src/js/libs",
    "src/less"
  ],
  "fileExtensions": [
    ".js"
  ],
  "validateIndentation": null,
  "validateQuoteMarks": "\"",
  "maximumLineLength": 500,
  "requireCamelCaseOrUpperCaseIdentifiers": null,
  "requireMultipleVarDecl": null,
  "disallowMultipleVarDecl": null,
  "requireCurlyBraces": null
}